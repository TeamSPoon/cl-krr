(every Hydrolysis-Acid-Reactant has-definition
       (instance-of (Chemical))
       (ka ((a Equilibrium-Constant-Value))))

(every Hydrolysis-Acid-Reactant has
  (called ("Reactant"))
  (raw-material-of (
    (if (not (the raw-material-of of Self))
	then
	(a Hydrolysis with
	   (raw-material (Self))
	   (ka ((the ka of Self))))
    )))
  (plays ((a Acid-Role with
	     (ka ((the ka of Self))))))
)
       
 
