                                 else (if (#'(LAMBDA () (CONCEPT-IS-DOMSPEC (CAR (KM0 '(|the| |classes| |of| (|the3| |of| |?trip|)))))))
                                       then
                                       (
                                        (?inst-index == (#'(LAMBDA () (INDEX-KB-INSTANCE (CAR (KM0 '(|the3| |of| |?trip|)))))))
                                        and
