				   (the Acid-Role plays of 
					(the first of (the base of (the input of Self)))))))
		   = *strong)
		  ;; because strong acid's [H+] = self's concentration
		  ;; even for H2SO4, so just use the self's concentration
		  ;and
		  ;((the second of (the base of (the input of Self))) isa Acid)
		  and
		  ((the1 of 
			 (the value of
			      (the intensity of 
				   (the Acid-Role plays of 
					(the second of (the base of (the input of Self)))))))
		   = *strong)
		  )
		then
	      ;; pH = -log([H+] + [H+])
	      (a PH-Value with
		 (value
		  (
		   (:pair
		    #'(LAMBDA 
		       ()
		       (LET ((X1 (FIRST
				  (KM0 '(the1 of
					      (the value of
						   (the concentration of
							(the first of 
							     (the base of 
								  (the input of Self)))))))))
			     (X2 (FIRST
				  (KM0 '(the1 of
					      (the value of
						   (the concentration of
							(the second of 
							     (the base of 
								  (the input of Self))))))))))
			    (IF (NOT (NUMBERP X1))
				(SETF X1 0))
			    (IF (NOT (NUMBERP X2))
				(SETF X2 0))
			    (IF (EQUAL (+ X1 X2) 0)
				NIL
				(- 0 (/ (LOG (+ X1 X2)) (LOG 10))))))
		    NIL))))))))
  (output (
	   ;; case 2: 2 strong bases
	   ((if ( ;((the first of (the base of (the input of Self))) isa Base)
		  ;and
		  ((the1 of 
			 (the value of
			      (the intensity of 
				   (the Base-Role plays of 
					(the first of 
