       (preparatory-event 
	(((:default
	     (if (has-value (the agent of Self))
		 then
		 (a Move with
		    (object      ((the agent of Self)))
		    (destination ((a Spatial-Entity with
				     (is-at ((the object of Self))))))
)))))))

