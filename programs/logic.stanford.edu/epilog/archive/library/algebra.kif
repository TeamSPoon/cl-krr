(<=> (group $s $f $g $e)     (and (closed $s $f)          (associative $f $s)          (identity $s $f $e)          (inverse $s $f $g $e)))(<=> (closed $s $f)     (forall ($x $y) (=> ($s $x) ($s $y) ($s ($f $x $y)))))(<=> (associative $f $s)     (forall ($x $y $z) (=> ($s $x) ($s $y) ($s $z)                            (<- ($f $x ($f $y $z)) ($f ($f $x $y) $z)))))(<=> (identity $s $f $e)     (forall $x (=> ($s $x) (and (<- ($f $x $e) $x) (<- ($f $e $x) $x)))))(<=> (inverse $s $f $g $e)     (forall $x (=> ($s $x) (and (<- ($f $x ($g $x)) $e)                                 (<- ($f ($g $x) $x) $e)))))