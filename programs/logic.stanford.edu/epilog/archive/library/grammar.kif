;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Intuitively simple but computationally inferior grammar;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;  (<= (sentence ?s)      (split ?np ?vp ?s)      (np ?np ?number nominative)      (vp ?vp ?number))  (<= (np (listof ?x) ?number ?case)      (pronoun ?x ?number ?case))  (<= (np (listof ?x) ?number ?case)      (proper-noun ?x ?number))  (<= (np (listof ?x @l) ?number ?case)      (determiner ?x ?number)      (rnp (listof @l) ?number))  (<= (rnp (listof ?x) ?number)      (noun ?x ?number))  (<= (rnp (listof ?x @l) ?number)      (adjective ?x)      (rnp (listof @l) ?number))  (<= (vp (listof ?v @l) ?number)      (verb ?v ?number)      (np (listof @l) ?other accusative))  (pronoun he singular nominative)  (pronoun she singular nominative)  (pronoun it singular nominative)  (pronoun i plural nominative)  (pronoun you plural nominative)  (pronoun we plural nominative)  (pronoun they plural nominative)  (pronoun me singular accusative)  (pronoun you singular accusative)  (pronoun him singular accusative)  (pronoun her singular accusative)  (pronoun it singular accusative)  (pronoun us singular accusative)  (pronoun them singular accusative)  (determiner a singular)  (determiner the singular)  (determiner one singular)  (determiner many plural)  (determiner most plural)  (adjective nice)  (adjective big)  (adjective old)  (proper-noun michael singular)  (proper-noun maureen singular)  (proper-noun computer-science singular)  (noun photograph singular)  (noun lamp singular)  (noun rabbit singular)  (verb likes singular)  (verb eats singular)  (verb like plural)  (verb eat plural)  (append (listof) ?m ?m)  (<= (append (listof ?x @l) ?m (listof ?x @n))      (append (listof @l) ?m (listof @n)))  (<= (split (listof ?x @l) (listof ?y @m) ?n)      (append  (listof ?x @l) (listof ?y @m) ?n))