;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Prove (not (innocent bertram));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(<= (friend bertram victor) (innocent arthur))(<= (not (likes carleton victor)) (innocent arthur))(<= (not (intown bertram)) (innocent bertram))(<= (not (knows bertram victor)) (innocent bertram))(<= (with arthur victor) (innocent carleton))(<= (with bertram victor) (innocent carleton))(<= (intown ?x) (with ?x victor))(<= (knows ?x ?y) (friend ?x ?y))(<= (knows ?x ?y) (likes ?x ?y))(or (innocent arthur) (innocent bertram))(or (innocent arthur) (innocent carleton))(or (innocent bertram) (innocent carleton))