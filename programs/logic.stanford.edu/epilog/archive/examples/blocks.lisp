is 5.NB: This problem category is difficult but doable.;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Problem a;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(deftheory blockst  (clear b s1)  (on b c s1)  (on c a s1)  (table a s1)    (<= (on ?x ?y (do (s ?x ?y) ?s))      (clear ?x ?s)      (table ?x ?s)      (clear ?y ?s))    (<= (on ?x ?y (do (s ?u ?v) ?s))      (on ?x ?y ?s))    (<= (clear ?x (do (s ?u ?v) ?s))      (clear ?x ?s)      (different ?x ?v))    (<= (table ?x (do (s ?u ?v) ?s))      (table ?x ?s)      (different ?x ?u))    (<= (clear ?y (do (u ?x ?y) ?s))      (clear ?x ?s)      (on ?x ?y ?s))    (<= (table ?x (do (u ?x ?y) ?s))      (clear ?x ?s)      (on ?x ?y ?s))    (<= (on ?x ?y (do (u ?u ?v) ?s))      (different ?u ?x)      (different ?v ?y)      (on ?x ?y ?s))    (<= (clear ?x (do (u ?u ?v) ?s))      (clear ?x ?s))    (<= (table ?x (do (u ?u ?v) ?s))      (table ?x ?s))    (different a b)  (different b a)  (different a c)  (different c a)  (different b c)  (different c b)    (<= (goal ?s) (on a b ?s) (on b c ?s)))(let ((*depth* 5)) (findx '?s '(goal ?s) 'blockst));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Results with 10000 step limit and depth 5;;;     54 lines of proof;;;     97 steps of search;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Problem a;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Premises;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;  (on c a s1)  (clear b s1)  (clear c s1)  (table a s1)  (table b s1)  (<= (goal ?s) (on a b ?s) (on b c ?s));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;