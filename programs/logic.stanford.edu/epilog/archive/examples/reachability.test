;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Reachability;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Background Premises;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(deftheory reachability ""  (reachable ?x ?x)  (<= (reachable ?x ?y) (path ?x ?y))  (<= (path ?x ?y)      (edge ?x ?y))  (<= (path ?x ?z)      (edge ?x ?y)      (path ?y ?z)))*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Problem a;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(deftheory reachabilitya ""  (edge a b)  (edge b c)  (edge c g)  (edge d e)  (edge e f))*(includes 'reachabilitya 'reachability)*(let ((*reduction* t) (*ancestry* t) (*depth* 20))  (fullfindp '(reachable a g) 'reachabilitya))T*inferences*9*unifications*50;;; *proof*;;; 14;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Problem b;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(deftheory reachabilityb ""  (edge a b)  (edge b c)  (edge c g)  (edge d e)  (edge e f))*(includes 'reachabilityb 'reachability)*(let ((*reduction* t) (*ancestry* t) (*depth* 20))  (fullfindp '(reachable a f) 'reachabilityb))NIL*inferences*13*unifications*108;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Problem c;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(deftheory reachabilityc ""  (edge a b)  (edge b c)  (edge c g)  (edge d e)  (edge e f)  (edge g a))*(includes 'reachabilityc 'reachability)*(let ((*reduction* t) (*ancestry* t) (*depth* 20))  (fullfindp '(reachable b a) 'reachabilityc))T*inferences*9*unifications*57;;; *proof*;;; 14;;;;;;;;;;;;;;;;;;;;;;;;;;;;;