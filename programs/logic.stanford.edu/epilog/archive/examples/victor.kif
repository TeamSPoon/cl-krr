;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Premises;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(=> (innocent arthur) (friend bertram victor))(=> (innocent arthur) (not (likes carleton victor)))(=> (innocent bertram) (not (intown bertram)))(=> (innocent bertram) (not (knows bertram victor)))(=> (innocent carleton) (with arthur victor))(=> (innocent carleton) (with bertram victor))(=> (with $x victor) (intown $x))(=> (friend $x $y) (knows $x $y))(=> (likes $x $y) (knows $x $y))(or (innocent arthur) (innocent bertram))(or (innocent arthur) (innocent carleton))(or (innocent bertram) (innocent carleton))(=> (not (innocent $x)) (ans $x));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; Show (not (innocent bertram));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(OR (NOT (INNOCENT ARTHUR)) (FRIEND BERTRAM VICTOR))(OR (NOT (INNOCENT ARTHUR)) (NOT (LIKES CARLETON VICTOR)))(OR (NOT (INNOCENT BERTRAM)) (NOT (INTOWN BERTRAM)))(OR (NOT (INNOCENT BERTRAM)) (NOT (KNOWS BERTRAM VICTOR)))(OR (NOT (INNOCENT CARLETON)) (WITH ARTHUR VICTOR))(OR (NOT (INNOCENT CARLETON)) (WITH BERTRAM VICTOR))(OR (NOT (WITH $X VICTOR)) (INTOWN $X))(OR (NOT (FRIEND $X $Y)) (KNOWS $X $Y))(OR (NOT (LIKES $X $Y)) (KNOWS $X $Y))(OR (INNOCENT ARTHUR) (INNOCENT BERTRAM))(OR (INNOCENT ARTHUR) (INNOCENT CARLETON))(OR (INNOCENT BERTRAM) (INNOCENT CARLETON))(OR (INNOCENT $X) (ANS $X))(OR (NOT (INNOCENT ARTHUR)) (KNOWS BERTRAM VICTOR))(OR (NOT (INNOCENT CARLETON)) (INTOWN BERTRAM))(OR (NOT (INNOCENT ARTHUR)) (NOT (INNOCENT BERTRAM)))(OR (NOT (INNOCENT CARLETON)) (NOT (INNOCENT BERTRAM)))(OR (INNOCENT CARLETON) (NOT (INNOCENT BERTRAM)))(NOT (INNOCENT BERTRAM))(ANS BERTRAM);;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;