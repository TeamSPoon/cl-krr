;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; introduction.test;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; reset;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(reset)*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; viewfindp, viewfindx, viewfinds;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(defparameter *global*  '((parent art bob)    (parent art bud)    (parent bob cal)    (parent bud coe)    (<= (grandparent ?x ?z) (parent ?x ?y) (parent ?y ?z))))*GLOBAL*;;;;(viewfindp '(parent art bob) *global*)T(viewfindp '(parent ?x bob) *global*)T(viewfindp '(parent ?x ?y) *global*)T;;;;(viewfindx '?x '(parent ?x bob) *global*)ART(viewfindx '?y '(parent art ?y) *global*)BOB(viewfindx '(related ?x ?y) '(parent ?x ?y) *global*)(RELATED ART BOB);;;;(viewfinds '?y '(parent art ?y) *global*)(BOB BUD)(viewfinds '(related ?x ?y) '(parent ?x ?y) *global*)((RELATED ART BOB) (RELATED ART BUD) (RELATED BOB CAL) (RELATED BUD COE));;;;(viewfindp '(grandparent art cal) *global*)T(viewfindx '?x '(grandparent ?x cal) *global*)ART(viewfindx '?y '(grandparent art ?y) *global*)CAL(viewfinds '?y '(grandparent art ?y) *global*)(CAL COE);;;;(defparameter *lisp*  '((succ 0 1)    (succ 1 2)    (succ 2 3)    (succ 3 4)    (length nil 0)    (<= (length (cons ?x ?l) ?n)        (length ?l ?n1)        (succ ?n1 ?n))    (append nil ?m ?m)     (<= (append (cons ?x ?l) ?m (cons ?x ?n))        (append ?l ?m ?n))))*(viewfindx '?n '(length (cons 1 (cons 2 (cons 3 nil))) ?n) *lisp*)3(viewfindx '?z '(append (cons 1 (cons 2 nil)) (cons 3 (cons 4 nil)) ?z) *lisp*)(cons 1 (cons 2 (cons 3 (cons 4 nil))));;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; reset;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;(reset)*;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;